    "babel-preset-env": "^1.6.1",
    "browser-sync": "^2.23.7",
    "concurrently": "^3.5.1",
    "discord-routes": "^1.0.7",
    "discord.js": "^11.3.2",
    "dotenv": "^5.0.1",
    "ejs": "^2.5.9",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "gulp": "^3.9.1",
    "gulp-live-server": "0.0.31",
    "lance-gg": "^2.0.5",
    "passport": "^0.4.0",
    "passport-discord": "^0.1.3",
    "passport-oauth2-refresh": "^1.0.0",
    "query-string": "^6.0.0",
